{
    "env": {
        "amd": true,
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react-hooks"
    ],
    "rules": {
        "react/display-name": "off",
        "react/prop-types": "off",
        "no-unused-vars": "warn",
        "react-hooks/rules-of-hooks": "error"
    }
}